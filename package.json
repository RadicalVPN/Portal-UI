    "@vueuse/core": "^10.6.1",
    "axios": "^1.6.5",
    "chart.js": "^3.9.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.30.0",
    "epic-spinners": "^2.0.0",
    "file-saver": "^2.0.5",
    "flag-icons": "^7.1.0",
    "ionicons": "^4.6.3",
    "pinia": "^2.0.16",
    "sass": "^1.53.0",
    "serve": "^14.2.0",
    "vue": "^3.3.4",
    "vue-chartjs": "^4.1.2",
    "vue-i18n": "^9.1.10",
